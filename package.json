{
	"scripts": {
		"start": "node src/index.js",
		"start:dev": "nodemon src/index.js",
		"seeding": "node src/seeds/userSeed.js",
		"build": "npm run clean && npm run copy-files && npm run compile",
		"clean": "rimraf dist",
		"compile": "babel src --out-dir dist --copy-files",
		"copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.json src/**/*.ejs src/**/*.pug dist/"
	},
	"dependencies": {
		"cors": "^2.8.6",
		"express": "^5.2.1",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.6",
		"@babel/core": "^7.28.6",
		"@babel/preset-env": "^7.28.6",
		"node-adodb": "^5.0.3",
		"copyfiles": "^2.4.1"
	}
}
